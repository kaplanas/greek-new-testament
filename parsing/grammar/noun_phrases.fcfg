######################
#### Building NPs ####
######################

#### Noun-like vocabulary items ####

# Nouns.
N_like[ADVERB=n, AGR=?agr, ARGS=?arg, CASE=?c, DE=?d, DEGREE=absolute, PAS=n, PROPER=?prop, REL=n, STANDALONE=n, WH=n] -> N[AGR=?agr, ARGS=?arg, CASE=?c, PROPER=?prop] C[DE=?d, SECOND_POSITION=y]

# Adjective phrases as nouns.
N_like[ADVERB=?adv, AGR=[GENDER=?g, NUMBER=?n, PERSON=3], ARGS=none, CASE=?c, DE=?d, DEGREE=?deg, PAS=?pas, PROPER=?prop, REL=n, STANDALONE=y, WH=?w] -> AP[ADVERB=?adv, AGR=[GENDER=?g, NUMBER=?n], CASE=?c, DE=?d, DEGREE=?deg, KAI=n, PAS=?pas, PROPER=?prop, STANDALONE=y, WH=?w]

#### Things that can modify nouns inside NPs ####

# Adjectives.
A_like[ADVERB=?adv, AGR=?agr, CASE=?c, DE=?d, DET_REQUIRED=n, PAS=?pas, WH=?w] -> AP[ADVERB=?adv, AGR=?agr, ATTRIBUTIVE=y, CASE=?c, DE=?d, PAS=?pas, WH=?w]
A_like[ADVERB=?adv, AGR=?agr, CASE=?c, DE=n, DET_REQUIRED=n, PAS=?pas, WH=?w]/AP[ADVERB=?adv, AGR=?agr, ATTRIBUTIVE=y, CASE=?apc, DE=?d, DEGREE=?deg, KAI=?k, PAS=?pas, PREDICATE=?pred, STANDALONE=?sa, WH=?w] -> AP/AP[ADVERB=?adv, AGR=?agr, ATTRIBUTIVE=y, CASE=?apc, DE=?d, DEGREE=?deg, KAI=?k, PAS=?pas, PREDICATE=?pred, STANDALONE=?sa, WH=?w]

# Genitive NPs.
A_like[ADVERB=n, DE=?d, DET_REQUIRED=n, PAS=n, WH=?w] -> NP[CASE=genitive, DE=?d, FUSED_REL=n, REL=n, WH=?w]

# PPs.
A_like[ADVERB=y, DE=?d, DET_REQUIRED=y, PAS=n, WH=?w] -> PP[DE=?d, WH=?w]

# Participles.
A_like[ADVERB=n, AGR=?agr, CASE=?c, DE=?d, DET_REQUIRED=n, PAS=n, WH=n] -> VP[AGR=?agr, CASE=?c, DE=?d, FINITE=n, MOOD=participle]

#### Single-constituent NPs ####

# Personal pronouns.
NP[ADVERB=n, AGR=?agr, CASE=?c, DE=?d, FUSED_REL=n, PAS=n, PREDICATE=n, PROPER=n, REL=n, STANDALONE=n, SUB_NP=n, WH=n] -> E[AGR=?agr, CASE=?c] C[DE=?d, SECOND_POSITION=y]

# Demonstrative pronouns.
NP[ADVERB=n, AGR=?agr, CASE=?c, DE=?d, FUSED_REL=n, PAS=n, PREDICATE=y, PROPER=n, REL=n, STANDALONE=n, SUB_NP=n, WH=n] -> M[AGR=?agr, CASE=?c] C[DE=?d, SECOND_POSITION=y]

# Indefinite and interrogative pronouns.  Only interrogatives can serve as predicates.
NP[ADVERB=n, AGR=?agr, CASE=?c, DE=?d, FUSED_REL=n, PAS=n, PREDICATE=?w, PROPER=n, REL=n, STANDALONE=n, SUB_NP=n, WH=?w] -> I[AGR=?agr, CASE=?c, WH=?w] C[DE=?d, SECOND_POSITION=y]

# Relative pronouns.
NP[ADVERB=n, AGR=?agr, CASE=?c, DE=?d, FUSED_REL=n, PAS=n, PREDICATE=n, PROPER=n, REL=y, STANDALONE=n, SUB_NP=n, WH=y] -> R[AGR=?agr, CASE=?c] C[DE=?d, SECOND_POSITION=y]

#### NPs with (mostly) ordinary nouns and possibly other things ####

# Bare noun.  If the "N-like" element is actually a standalone adjective, flag this; we don't want to allow NPs like
# this as predicates, because that's already covered by AP predicates.
NP[ADVERB=?adv, AGR=[GENDER=?g, NUMBER=?n, PERSON=3], CASE=?c, DE=?d, FUSED_REL=n, PAS=?pas, PREDICATE=y, PROPER=?prop, REL=?r, STANDALONE=?sa, SUB_NP=n, WH=?w] -> N_like[ADVERB=?adv, AGR=[GENDER=?g, NUMBER=?n, PERSON=3], CASE=?c, DE=?d, PAS=?pas, PROPER=?prop, REL=?r, STANDALONE=?sa, WH=?w]

# Determiner + noun.  Participles here (must be accompanied by a determiner).
NP[ADVERB=?adv, AGR=[GENDER=?g, NUMBER=?n, PERSON=3], CASE=?c, DE=?d, FUSED_REL=n, PAS=?pas, PREDICATE=y, PROPER=?prop, REL=?r, STANDALONE=n, SUB_NP=n, WH=n] -> D[AGR=?agr, CASE=?c] C[DE=?d, SECOND_POSITION=y] N_like[ADVERB=?adv, AGR=?agr[GENDER=?g, NUMBER=?n, PERSON=3], CASE=?c, DE=n, PAS=?pas, PROPER=?prop, REL=?r, WH=n]
NP[ADVERB=n, AGR=?agr, CASE=?c, DE=?d, FUSED_REL=n, PAS=n, PREDICATE=y, PROPER=n, REL=n, STANDALONE=n, SUB_NP=n, WH=n] -> D[AGR=?agr, CASE=?c] C[DE=?d, SECOND_POSITION=y] VP[ADVERB=?adv, AGR=?agr, CASE=?c, DE=n, FINITE=n, MOOD=participle, WH=n]
NP[ADVERB=n, AGR=?agr, CASE=?c, DE=?d, FUSED_REL=n, PAS=n, PREDICATE=y, PROPER=n, REL=n, STANDALONE=n, SUB_NP=n, WH=n] -> D[AGR=?agr, CASE=?c] C[DE=?d, SECOND_POSITION=y] M[AGR=?agr, CASE=?c]

# Adjective + noun, in either order.
NP[ADVERB=?adv, AGR=?agr[GENDER=?g, NUMBER=?n], CASE=?c, DE=?d, FUSED_REL=n, PAS=n, PREDICATE=y, PROPER=n, REL=?r, STANDALONE=n, SUB_NP=n, WH=?w] -> A_like[AGR=[GENDER=?g, NUMBER=?n], CASE=?c, DE=?d, DET_REQUIRED=n, PAS=n, WH=n] N_like[ADVERB=?adv, AGR=?agr[GENDER=?g, NUMBER=?n], CASE=?c, DE=n, PAS=n, PROPER=n, REL=?r, WH=?w]
NP[ADVERB=?adv, AGR=?agr[GENDER=?g, NUMBER=?n], CASE=?c, DE=?d, FUSED_REL=n, PAS=n, PREDICATE=y, PROPER=n, REL=?r, STANDALONE=n, SUB_NP=n, WH=y] -> A_like[AGR=[GENDER=?g, NUMBER=?n], CASE=?c, DE=?d, DET_REQUIRED=n, WH=y] N_like[ADVERB=?adv, AGR=?agr[GENDER=?g, NUMBER=?n], CASE=?c, DE=n, PAS=n, PROPER=n, REL=?r, WH=n]
NP[ADVERB=?adv, AGR=?agr[GENDER=?g, NUMBER=?n], CASE=?c, DE=?d, FUSED_REL=n, PAS=n, PREDICATE=y, PROPER=n, REL=?r, STANDALONE=n, SUB_NP=n, WH=?w] -> N_like[ADVERB=?adv, AGR=?agr[GENDER=?g, NUMBER=?n], CASE=?c, DE=?d, PAS=n, PROPER=n, REL=?r, WH=?w] A_like[AGR=[GENDER=?g, NUMBER=?n], CASE=?c, DE=n, WH=n]

# Two adjectives.
NP[ADVERB=?adv, AGR=?agr[GENDER=?g, NUMBER=?n, PERSON=?p], CASE=?c, DE=n, FUSED_REL=n, PAS=n, PREDICATE=y, PROPER=n, REL=?r, STANDALONE=n, SUB_NP=n, WH=?w]/AP[AGR=[GENDER=?g, NUMBER=?n], ADVERB=?apadv, ATTRIBUTIVE=?att, CASE=?apc, DE=?d, DEGREE=?deg, KAI=?k, PAS=?pas, PREDICATE=?pred, STANDALONE=?sa, WH=?apw] -> A_like[AGR=[GENDER=?g, NUMBER=?n], CASE=?c]/AP[AGR=[GENDER=?g, NUMBER=?n], ADVERB=?apadv, ATTRIBUTIVE=?att, CASE=?apc, DE=?d, DEGREE=?deg, KAI=?k, PAS=?pas, PREDICATE=?pred, STANDALONE=?sa, WH=?apw] N_like[ADVERB=?adv, AGR=[GENDER=?g, NUMBER=?n, PERSON=?p], CASE=?c, DE=n, PAS=n, PROPER=n, REL=?r, WH=?w] A_like[AGR=[GENDER=?g, NUMBER=?n], CASE=?c, DE=n, WH=n]

# Determiner + adjective + noun, in some order.
NP[ADVERB=?adv, AGR=?agr[GENDER=?g, NUMBER=?n], CASE=?c, DE=?d, FUSED_REL=n, PAS=?pas, PREDICATE=y, PROPER=?prop, REL=?r, STANDALONE=n, SUB_NP=n, WH=n] -> D[AGR=[GENDER=?g, NUMBER=?n], CASE=?c] C[DE=?d, SECOND_POSITION=y] A_like[AGR=[GENDER=?g, NUMBER=?n], CASE=?c, DE=n, WH=n] N_like[ADVERB=?adv, AGR=?agr, CASE=?c, DE=n, PAS=?pas, PROPER=?prop, REL=?r, WH=n]
NP[ADVERB=?adv, AGR=?agr[GENDER=?g, NUMBER=?n], CASE=?c, DE=?d, FUSED_REL=n, PAS=?pas, PREDICATE=y, PROPER=?prop, REL=?r, STANDALONE=n, SUB_NP=n, WH=n] -> D[AGR=[GENDER=?g, NUMBER=?n], CASE=?c] C[DE=?d, SECOND_POSITION=y] N_like[ADVERB=?adv, AGR=?agr, CASE=?c, DE=n, PAS=?pas, PROPER=?prop, REL=?r, WH=n] A_like[AGR=[GENDER=?g, NUMBER=?n], CASE=?c, DE=n, WH=n]

# A genitive pronoun, but nothing else, can come before the determiner.
NP[ADVERB=?adv, AGR=[GENDER=?g, NUMBER=?n, PERSON=3], CASE=?c, DE=?d, FUSED_REL=n, PAS=?pas, PREDICATE=y, PROPER=?prop, REL=?r, STANDALONE=n, SUB_NP=n, WH=n] -> E[CASE=genitive, DE=?d] D[AGR=?agr, CASE=?c] N_like[ADVERB=?adv, AGR=?agr[GENDER=?g, NUMBER=?n, PERSON=3], CASE=?c, DE=n, PAS=?pas, PROPER=?prop, REL=?r, WH=n]
NP[ADVERB=?adv, AGR=[GENDER=?g, NUMBER=?n, PERSON=3], CASE=?c, DE=?d, FUSED_REL=n, PAS=?pas, PREDICATE=y, PROPER=?prop, REL=y, STANDALONE=n, SUB_NP=n, WH=n] -> R[CASE=genitive, DE=?d] D[AGR=?agr, CASE=?c] N_like[ADVERB=?adv, AGR=?agr[GENDER=?g, NUMBER=?n, PERSON=3], CASE=?c, DE=n, PAS=?pas, PROPER=?prop, REL=n, WH=n]

# Determiner + noun + determiner + adjective.
NP[ADVERB=?adv, AGR=?agr[GENDER=?g, NUMBER=?n], CASE=?c, DE=?d, FUSED_REL=n, PAS=?pas, PREDICATE=y, PROPER=?prop, REL=?r, STANDALONE=n, SUB_NP=n, WH=n] -> D[AGR=[GENDER=?g, NUMBER=?n], CASE=?c] C[DE=?d, SECOND_POSITION=y] N_like[ADVERB=?adv, AGR=?agr, CASE=?c, DE=n, PAS=?pas, PROPER=?prop, REL=?r, WH=n] D[AGR=[GENDER=?g, NUMBER=?n], CASE=?c] A_like[AGR=[GENDER=?g, NUMBER=?n], CASE=?c, DE=n, WH=n]

# Determiner + noun + demonstrative, in some order.
NP[ADVERB=?adv, AGR=?agr[GENDER=?g, NUMBER=?n], CASE=?c, DE=?d, FUSED_REL=n, PAS=?pas, PREDICATE=y, PROPER=?prop, REL=?r, STANDALONE=n, SUB_NP=n, WH=n] -> D[AGR=[GENDER=?g, NUMBER=?n], CASE=?c] C[DE=?d, SECOND_POSITION=y] N_like[ADVERB=?adv, AGR=[GENDER=?g, NUMBER=?n], CASE=?c, DE=n, PAS=?pas, PROPER=?prop, REL=?r, WH=n] M[AGR=[GENDER=?g, NUMBER=?n], CASE=?c, DE=n, WH=n]
NP[ADVERB=?adv, AGR=?agr[GENDER=?g, NUMBER=?n], CASE=?c, DE=?d, FUSED_REL=n, PAS=?pas, PREDICATE=y, PROPER=?prop, REL=?r, STANDALONE=n, SUB_NP=n, WH=n] -> M[AGR=[GENDER=?g, NUMBER=?n], CASE=?c, DE=n, WH=n] C[DE=?d, SECOND_POSITION=y] D[AGR=[GENDER=?g, NUMBER=?n], CASE=?c] N_like[ADVERB=?adv, AGR=[GENDER=?g, NUMBER=?n], CASE=?c, DE=n, PAS=?pas, PROPER=?prop, REL=?r, WH=n]

# Determiner + noun + demonstrative + adjective.
NP[ADVERB=?adv, AGR=?agr[GENDER=?g, NUMBER=?n], CASE=?c, DE=?d, FUSED_REL=n, PAS=?pas, PREDICATE=y, PROPER=?prop, REL=?r, STANDALONE=n, SUB_NP=n, WH=n] -> M[AGR=[GENDER=?g, NUMBER=?n], CASE=?c, DE=n, WH=n] C[DE=?d, SECOND_POSITION=y] D[AGR=[GENDER=?g, NUMBER=?n], CASE=?c] A_like[AGR=[GENDER=?g, NUMBER=?n], CASE=?c, DE=n, WH=n] N_like[ADVERB=?adv, AGR=[GENDER=?g, NUMBER=?n], CASE=?c, DE=n, PAS=?pas, PROPER=?prop, REL=?r, WH=n]

#### Fused relatives ####

# NP fronted out of IP as its own special case, regardless of the case of the fronted NP.  This means we have to
# duplicate a bunch of VP rules, but the alternative is massive ambiguity (e.g., a verb that can take either an
# accusative or a dative argument will end up with two parses, one for each possible case of the fused relative).
NP[AGR=[GENDER=?g, NUMBER=?n, PERSON=3], CASE=fused_rel, DE=?d, FUSED_REL=y, PAS=?pas, PROPER=?prop, REL=?r, STANDALONE=n, SUB_NP=n, WH=n] -> NP[AGR=?agr[GENDER=?g, NUMBER=?n], CASE=?c, DE=?d, PAS=?pas, PROPER=?prop, REL=?r, WH=y] IP[DE=n, VERBLESS=n]/NP[AGR=?agr, CASE=?c, PROPER=?prop, REL=?r, WH=y]

# Subject NP; always nominative.
NP[AGR=?agr, CASE=nominative, DE=?d, FUSED_REL=y, PAS=?pas, PROPER=?prop, REL=?r, STANDALONE=n, SUB_NP=n, WH=n] -> IP[AGR=?agr, DE=?d, REL=y, SUBJECT=y, VERBLESS=n]

#### Infinitives ####

# o + infinitive + accusative subject
NP[ADVERB=n, AGR=[GENDER=neuter, NUMBER=singular], CASE=?c, DE=?d, FUSED_REL=n, PAS=n, POS=NP, PREDICATE=n, PROPER=n, REL=n, STANDALONE=n, SUB_NP=n, WH=n] -> D[AGR=[GENDER=neuter, NUMBER=singular], CASE=?c] C[DE=?d, SECOND_POSITION=y] VP[DE=n, MOOD=infinitive, VERBLESS=n] NP[CASE=accusative, DE=n, WH=n]

#### Miscellaneous constructions ####

# outos + pas
NP[ADVERB=n, AGR=?agr, CASE=?c, DE=?d, FUSED_REL=n, PAS=n, PREDICATE=n, PROPER=n, REL=n, STANDALONE=n, SUB_NP=n, WH=n] -> M[AGR=?agr, CASE=?c] C[DE=?d, SECOND_POSITION=y] A[AGR=?agr, CASE=?c, PAS=y, WH=n]

# ta + PP
NP[ADVERB=n, AGR=[GENDER=neuter, NUMBER=plural], CASE=?c, DE=?d, FUSED_REL=n, PAS=n, PREDICATE=n, PROPER=n, REL=n, STANDALONE=n, SUB_NP=n, WH=n] -> D[AGR=[GENDER=neuter, NUMBER=plural], CASE=?c] C[DE=?d, SECOND_POSITION=y] PP

# Infinitive argument.
NP[AGR=?agr, CASE=?c, DE=?d, FUSED_REL=n, PAS=n, PROPER=n, REL=n, STANDALONE=n, SUB_NP=n, WH=n] -> N_like[AGR=?agr, ARGS=[ARG_INF=y], CASE=?c, DE=?d, PAS=n, PROPER=n, REL=n, WH=n] VP[FINITE=n, MOOD=infinitive, VERBLESS=n]

#### Movement out of NPs ####

# Head N moved out.
NP[ADVERB=n, AGR=?agr[GENDER=?g, NUMBER=?n], CASE=?c, DE=?d, FUSED_REL=n, PAS=n, PREDICATE=y, PROPER=n, REL=n, STANDALONE=n, SUB_NP=n, WH=n]/N[AGR=?agr[GENDER=?g, NUMBER=?n], CASE=?c, PROPER=n] -> N/N[AGR=?agr[GENDER=?g, NUMBER=?n], CASE=?c, PROPER=n] C[DE=?d, SECOND_POSITION=y] A_like[AGR=[GENDER=?g, NUMBER=?n], CASE=?c, DE=n, WH=n]
NP[ADVERB=?adv, AGR=?agr, CASE=?c, DE=?d, FUSED_REL=n, PAS=n, PREDICATE=y, PROPER=n, REL=?r, STANDALONE=n, SUB_NP=n, WH=?w]/N_like[ADVERB=?adv, AGR=?agr, ARGS=?arg, CASE=?c, DE=?d, PAS=n, PROPER=n, REL=?r, WH=?w] -> N_like/N_like[ADVERB=?adv, AGR=?agr, ARGS=?arg[ARG_INF=y], CASE=?c, DE=?d, PAS=n, PROPER=n, REL=?r, WH=?w] VP[FINITE=n, MOOD=infinitive, VERBLESS=n]

# APs moved out of NPs.
NP[ADVERB=?adv, AGR=?agr, CASE=?c, DE=n, FUSED_REL=n, PAS=n, PREDICATE=y, PROPER=n, REL=?r, STANDALONE=n, SUB_NP=n, WH=n]/AP[ADVERB=?apadv, AGR=?apagr, ATTRIBUTIVE=?att, CASE=?apc, DE=?d, DEGREE=?deg, KAI=?k, PAS=n, PREDICATE=?pred, STANDALONE=?sa, WH=?w] -> N_like[ADVERB=?adv, AGR=?agr, CASE=?c, DE=n, PAS=n, PROPER=n, REL=?r, WH=n] AP/AP[ADVERB=?apadv, AGR=?apagr, ATTRIBUTIVE=?att, CASE=?apc, DE=?d, DEGREE=?deg, KAI=?k, PAS=n, PREDICATE=?pred, STANDALONE=?sa, WH=?w]

# Genitive NPs moved out of NPs.
NP[ADVERB=?adv, AGR=?agr, CASE=?c, DE=n, FUSED_REL=n, PAS=n, PREDICATE=y, PROPER=n, REL=?r, STANDALONE=n, SUB_NP=y, WH=n]/NP[ADVERB=?npadv, AGR=?npagr, CASE=genitive, DE=?d, FUSED_REL=n, PAS=n, PREDICATE=?pred, PROPER=?prop, REL=n, STANDALONE=n, SUB_NP=n, WH=y] -> N_like[ADVERB=?adv, AGR=?agr, CASE=?c, DE=n, PAS=n, PROPER=n, REL=?r, WH=n] NP/NP[ADVERB=?npadv, AGR=?npagr, CASE=genitive, DE=?d, FUSED_REL=n, PAS=n, PREDICATE=?pred, PROPER=?prop, REL=n, STANDALONE=n, SUB_NP=n, WH=y]

