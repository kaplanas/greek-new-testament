NP[AGR=[NUMBER=plural], CASE=?c, DE=?d, FUSED_REL=n, PAS=n, PREDICATE=y, PROPER=n, REL=n, STANDALONE=n, SUB_NP=y, WH=n] -> NP[CASE=?c, DE=?d, FUSED_REL=n, PAS=n, REL=n, WH=n] C[SECOND_POSITION=n] NP[CASE=?c, DE=n, FUSED_REL=n, PAS=n, REL=n, WH=n]

