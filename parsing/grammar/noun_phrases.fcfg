NP[AGR=?agr, CASE=?c, DE=n, FUSED_REL=n, PAS=n, PROPER=y, REL=?r, STANDALONE=n, SUB_NP=n, WH=n] -> N[AGR=?agr, CASE=?c, PROPER=y] PP
NP[AGR=?agr, CASE=dative, DE=n, FUSED_REL=n, PAS=n, PROPER=y, REL=n, STANDALONE=n, SUB_NP=n, WH=n] -> N[AGR=?agr, CASE=dative, PROPER=y] NP[CASE=genitive, DE=n, REL=n, WH=n]
NP[AGR=?agr, CASE=?c, DE=n, FUSED_REL=n, PAS=n, PROPER=y, REL=?r, STANDALONE=n, SUB_NP=n, WH=n] -> NP[AGR=?agr, CASE=?c, DE=n, PROPER=n, REL=?r, WH=n] N[AGR=?agr, CASE=?c, PROPER=y]

# Indefinites modified by adjectives.
NP[ADVERB=n, AGR=[GENDER=?g, NUMBER=?n], CASE=?c, DE=?d, FUSED_REL=n, PAS=n, PROPER=n, REL=n, STANDALONE=n, SUB_NP=n, WH=n] -> I[AGR=[GENDER=?g, NUMBER=?n], CASE=?c, WH=n] C[DE=?d, SECOND_POSITION=y] A_like[AGR=[GENDER=?g, NUMBER=?n], CASE=?c, PAS=n]

###########################
#### Adjective phrases ####
###########################

#### Bare adjectives ####

# Bare adjective.
AP[ADVERB=?adv, AGR=?agr, ATTRIBUTIVE=?att, CASE=?c, DE=?d, DEGREE=?deg, KAI=n, PAS=?pas, PREDICATE=?pred, STANDALONE=?sa, WH=?w] -> A[ADVERB=?adv, AGR=?agr, ATTRIBUTIVE=?att, CASE=?c, DEGREE=?deg, PAS=?pas, PREDICATE=?pred, STANDALONE=?sa, WH=?w] C[DE=?d, SECOND_POSITION=y]

#### Adjectives with arguments ####

# Genitive argument.
AP[ADVERB=?adv, AGR=?agr, ATTRIBUTIVE=?att, CASE=?c, DE=?d, DEGREE=?deg, KAI=n, PAS=?pas, PREDICATE=?pred, STANDALONE=?sa, WH=?w] -> NP[CASE=genitive, DE=?d] A[ADVERB=?adv, AGR=?agr, ARGS=[ARG_GEN=y], ATTRIBUTIVE=?att, CASE=?c, DEGREE=?deg, PAS=?pas, PREDICATE=?pred, STANDALONE=?sa, WH=?w]

# Genitive after a comparative.
AP[ADVERB=?adv, AGR=?agr, ATTRIBUTIVE=?att, CASE=?c, DE=?d, DEGREE=comparative, KAI=n, PAS=?pas, PREDICATE=?pred, STANDALONE=?sa, WH=?w] -> A[ADVERB=?adv, AGR=?agr, ATTRIBUTIVE=?att, CASE=?c, DEGREE=comparative, PAS=?pas, PREDICATE=?pred, STANDALONE=?sa, WH=?w] NP[CASE=genitive, DE=?d]

#### Pre-AP particles ####

# kai.
AP[ADVERB=?adv, AGR=?agr, ATTRIBUTIVE=?att, CASE=?c, DE=?d, DEGREE=?deg, KAI=n, PAS=?pas, PREDICATE=?pred, STANDALONE=n, WH=?w] -> 'kai/_B-------' A[ADVERB=?adv, AGR=?agr, ATTRIBUTIVE=?att, CASE=?c, DEGREE=?deg, PAS=?pas, PREDICATE=?pred, STANDALONE=n, WH=?w] C[DE=?d, SECOND_POSITION=y]

###########################
#### Predicate phrases ####
###########################

#### Building predicates ####

# (Usually) nominative noun or adjective phrase.
PredP[CASE=?c, DE=?d, WH=?w] -> NP[CASE=?c, DE=?d, FUSED_REL=n, PREDICATE=y, STANDALONE=n, WH=?w]
PredP[AGR=[GENDER=?g, NUMBER=?n], CASE=?c, DE=?d, WH=n] -> AP[AGR=[GENDER=?g, NUMBER=?n], CASE=?c, DE=?d, PREDICATE=y]

# Dative noun phrase.
PredP[CASE=dative, DE=?d, WH=?w] -> NP[CASE=dative, DE=?d, FUSED_REL=n, PREDICATE=n, STANDALONE=n, WH=?w]

# Genitive noun phrase.
PredP[CASE=genitive, DE=?d, WH=?w] -> NP[CASE=genitive, DE=?d, FUSED_REL=n, PREDICATE=n, STANDALONE=n, WH=?w]

# Adverb phrase.  Don't allow PPs with pros, because there's a special discourse construction that allows them and
# they're not needed as predicates elsewhere.
PredP[CASE=none, DE=?d, WH=?w] -> AdvP[DE=?d, PREDICATE=y, PROS=n, WH=?w]

# Participle.
PredP[AGR=[GENDER=?g, NUMBER=?n], CASE=?c, DE=?d, WH=n] -> VP[AGR=[GENDER=?g, NUMBER=?n], CASE=?c, DE=?d, FINITE=n, MOOD=participle, VERBLESS=n, WH=n]

# Infinitive.
PredP[AGR=[GENDER=neuter, NUMBER=s, PERSON=3], CASE=none, DE=?d, WH=n] -> VP[DE=?d, FINITE=n, MOOD=infinitive, VERBLESS=n, WH=n]

# Dative noun phrase + nominative noun phrase.
PredP[CASE=nominative, DE=?d, WH=?w] -> NP[CASE=dative, DE=?d, FUSED_REL=n, PREDICATE=n, STANDALONE=n, WH=n] NP[CASE=nominative, DE=n, FUSED_REL=n, PREDICATE=y, STANDALONE=n, WH=?w]

#### Moving things out of predicates ####

# Adjectives.
PredP[CASE=?c, DE=?d, WH=?w]/AP[AGR=?agr, ADVERB=?adv, ATTRIBUTIVE=?att, CASE=?apc, DE=?d, DEGREE=?deg, KAI=?k, PAS=?pas, PREDICATE=?pred, STANDALONE=?sa, WH=?apw] -> NP[CASE=?c, DE=?d, FUSED_REL=n, PREDICATE=y, STANDALONE=n, WH=?w]/AP[AGR=?agr, ADVERB=?adv, ATTRIBUTIVE=?att, CASE=?apc, DE=?d, DEGREE=?deg, KAI=?k, PAS=?pas, PREDICATE=?pred, STANDALONE=?sa, WH=?apw]

# Genitive NPs.
PredP[CASE=?c, DE=?d, WH=n]/NP[ADVERB=?npadv, AGR=?npagr, CASE=genitive, DE=?d, FUSED_REL=n, PAS=n, PREDICATE=?pred, PROPER=?prop, REL=n, STANDALONE=n, SUB_NP=n, WH=y] -> NP[CASE=?c, DE=?d, FUSED_REL=n, PREDICATE=y, STANDALONE=n, SUB_NP=y, WH=n]/NP[ADVERB=?npadv, AGR=?npagr, CASE=genitive, DE=?d, FUSED_REL=n, PAS=n, PREDICATE=?pred, PROPER=?prop, REL=n, STANDALONE=n, SUB_NP=n, WH=y]
