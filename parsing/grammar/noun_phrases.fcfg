######################
#### Building NPs ####
######################

#### Noun-like vocabulary items ####

# Nouns.
N_like[ADVERB=n, AGR=?agr, ARGS=?arg, CASE=?c, DE=?d, DEGREE=absolute, PAS=n, PROPER=?prop, REL=n, STANDALONE=n, WH=n] -> N[AGR=?agr, ARGS=?arg, CASE=?c, PROPER=?prop] C[DE=?d, SECOND_POSITION=y]

# Adjective phrases as nouns.
N_like[ADVERB=?adv, AGR=[GENDER=?g, NUMBER=?n, PERSON=3], ARGS=none, CASE=?c, DE=?d, DEGREE=?deg, PAS=?pas, PROPER=?prop, REL=n, STANDALONE=y, WH=?w] -> AP[ADVERB=?adv, AGR=[GENDER=?g, NUMBER=?n], CASE=?c, DE=?d, DEGREE=?deg, KAI=n, PAS=?pas, PROPER=?prop, STANDALONE=y, WH=?w]

#### Things that can modify nouns inside NPs ####

# Adjectives.
A_like[ADVERB=?adv, AGR=?agr, CASE=?c, DE=?d, DET_REQUIRED=n, PAS=?pas, WH=?w] -> AP[ADVERB=?adv, AGR=?agr, ATTRIBUTIVE=y, CASE=?c, DE=?d, PAS=?pas, WH=?w]
A_like[ADVERB=?adv, AGR=?agr, CASE=?c, DE=n, DET_REQUIRED=n, PAS=?pas, WH=?w]/AP[ADVERB=?adv, AGR=?apagr, ATTRIBUTIVE=y, CASE=?apc, DE=?d, DEGREE=?deg, KAI=?k, PAS=?pas, PREDICATE=?pred, STANDALONE=?sa, WH=?w] -> AP/AP[ADVERB=?adv, AGR=?apagr, ATTRIBUTIVE=y, CASE=?apc, DE=?d, DEGREE=?deg, KAI=?k, PAS=?pas, PREDICATE=?pred, STANDALONE=?sa, WH=?w]

# Genitive NPs.
A_like[ADVERB=n, DE=?d, DET_REQUIRED=n, PAS=n, WH=?w] -> NP[CASE=genitive, DE=?d, FUSED_REL=n, REL=n, WH=?w]

# PPs.
A_like[ADVERB=y, DE=?d, DET_REQUIRED=y, PAS=n, WH=?w] -> PP[DE=?d, WH=?w]

# Participles.
A_like[ADVERB=n, AGR=?agr, CASE=?c, DE=?d, DET_REQUIRED=n, PAS=n, WH=n] -> VP[AGR=?agr, CASE=?c, DE=?d, FINITE=n, MOOD=participle]

#### Single-constituent NPs ####

# Personal pronouns.
NP[ADVERB=n, AGR=?agr, CASE=?c, DE=?d, FUSED_REL=n, PAS=n, PREDICATE=n, PROPER=n, REL=n, STANDALONE=n, SUB_NP=n, WH=n] -> E[AGR=?agr, CASE=?c] C[DE=?d, SECOND_POSITION=y]

# Demonstrative pronouns.
NP[ADVERB=n, AGR=?agr, CASE=?c, DE=?d, FUSED_REL=n, PAS=n, PREDICATE=y, PROPER=n, REL=n, STANDALONE=n, SUB_NP=n, WH=n] -> M[AGR=?agr, CASE=?c] C[DE=?d, SECOND_POSITION=y]

# Indefinite and interrogative pronouns.  Only interrogatives can serve as predicates.
NP[ADVERB=n, AGR=?agr, CASE=?c, DE=?d, FUSED_REL=n, PAS=n, PREDICATE=?w, PROPER=n, REL=n, STANDALONE=n, SUB_NP=n, WH=?w] -> I[AGR=?agr, CASE=?c, WH=?w] C[DE=?d, SECOND_POSITION=y]

# Relative pronouns.
NP[ADVERB=n, AGR=?agr, CASE=?c, DE=?d, FUSED_REL=n, PAS=n, PREDICATE=n, PROPER=n, REL=y, STANDALONE=n, SUB_NP=n, WH=y] -> R[AGR=?agr, CASE=?c] C[DE=?d, SECOND_POSITION=y]

#### NPs with (mostly) ordinary nouns and possibly other things ####

# Bare noun.  If the "N-like" element is actually a standalone adjective, flag this; we don't want to allow NPs like
# this as predicates, because that's already covered by AP predicates.
NP[ADVERB=?adv, AGR=[GENDER=?g, NUMBER=?n, PERSON=3], CASE=?c, DE=?d, FUSED_REL=n, PAS=?pas, PREDICATE=y, PROPER=?prop, REL=?r, STANDALONE=?sa, SUB_NP=n, WH=?w] -> N_like[ADVERB=?adv, AGR=[GENDER=?g, NUMBER=?n, PERSON=3], CASE=?c, DE=?d, PAS=?pas, PROPER=?prop, REL=?r, STANDALONE=?sa, WH=?w]

# Determiner + noun.  Participles here (must be accompanied by a determiner).
NP[ADVERB=?adv, AGR=[GENDER=?g, NUMBER=?n, PERSON=3], CASE=?c, DE=?d, FUSED_REL=n, PAS=?pas, PREDICATE=y, PROPER=?prop, REL=?r, STANDALONE=n, SUB_NP=n, WH=n] -> D[AGR=?agr, CASE=?c] C[DE=?d, SECOND_POSITION=y] N_like[ADVERB=?adv, AGR=?agr[GENDER=?g, NUMBER=?n, PERSON=3], CASE=?c, DE=n, PAS=?pas, PROPER=?prop, REL=?r, WH=n]
NP[ADVERB=n, AGR=?agr, CASE=?c, DE=?d, FUSED_REL=n, PAS=n, PREDICATE=y, PROPER=n, REL=n, STANDALONE=n, SUB_NP=n, WH=n] -> D[AGR=?agr, CASE=?c] C[DE=?d, SECOND_POSITION=y] VP[ADVERB=?adv, AGR=?agr, CASE=?c, DE=n, FINITE=n, MOOD=participle, WH=n]
NP[ADVERB=n, AGR=?agr, CASE=?c, DE=?d, FUSED_REL=n, PAS=n, PREDICATE=y, PROPER=n, REL=n, STANDALONE=n, SUB_NP=n, WH=n] -> D[AGR=?agr, CASE=?c] C[DE=?d, SECOND_POSITION=y] M[AGR=?agr, CASE=?c]

# Adjective + noun, in either order.
NP[ADVERB=?adv, AGR=?agr[GENDER=?g, NUMBER=?n], CASE=?c, DE=?d, FUSED_REL=n, PAS=n, PREDICATE=y, PROPER=n, REL=?r, STANDALONE=n, SUB_NP=n, WH=?w] -> A_like[AGR=[GENDER=?g, NUMBER=?n], CASE=?c, DE=?d, DET_REQUIRED=n, PAS=n, WH=n] N_like[ADVERB=?adv, AGR=?agr[GENDER=?g, NUMBER=?n], CASE=?c, DE=n, PAS=n, PROPER=n, REL=?r, WH=?w]
NP[ADVERB=?adv, AGR=?agr[GENDER=?g, NUMBER=?n], CASE=?c, DE=?d, FUSED_REL=n, PAS=n, PREDICATE=y, PROPER=n, REL=?r, STANDALONE=n, SUB_NP=n, WH=y] -> A_like[AGR=[GENDER=?g, NUMBER=?n], CASE=?c, DE=?d, DET_REQUIRED=n, WH=y] N_like[ADVERB=?adv, AGR=?agr[GENDER=?g, NUMBER=?n], CASE=?c, DE=n, PAS=n, PROPER=n, REL=?r, WH=n]
